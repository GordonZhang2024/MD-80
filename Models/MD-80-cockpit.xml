	<animation>
		<type>translate</type>
		<object-name>FlapLever</object-name>
		<property>/controls/cockpit/flaps-lever-translate</property>
		<factor>0.01</factor>
		<axis>
			<x1-m>2.56535</x1-m>
			<y1-m>0</y1-m>
			<z1-m>0.71202</z1-m>
			<x2-m>2.53992</x2-m>
			<y2-m>0</y2-m>
			<z2-m>0.74234</z2-m>
		</axis>
	</animation>
	
	<animation>
		<type>knob</type>
		<object-name>FlapLever</object-name>
		<drag-direction>vertical</drag-direction>
		<drag-scale-px>30</drag-scale-px>
		<increase>
			<binding>
				<command>nasal</command>
				<script>controls.flapsDown(-1);</script>
			</binding>
			<binding>
				<command>property-assign</command>
				<property>/controls/cockpit/flaps-lever-translate-cmd</property>
				<value>1</value>
			</binding>
		</increase>
		<decrease>
			<binding>
				<command>nasal</command>
				<script>controls.flapsDown(1);</script>
			</binding>
			<binding>
				<command>property-assign</command>
				<property>/controls/cockpit/flaps-lever-translate-cmd</property>
				<value>1</value>
			</binding>
		</decrease>
		<release>
			<binding>
				<command>property-assign</command>
				<property>/controls/cockpit/flaps-lever-translate-cmd</property>
				<value>0</value>
			</binding>
		</release>
	</animation>
	
