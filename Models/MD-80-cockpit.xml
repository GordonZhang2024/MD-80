					</greater-than-equals>
				</condition>
				<command>property-adjust</command>
				<property>controls/flight/elevator-trim</property>
				<step>0.001</step>
			</binding>
			<binding>
				<command>property-assign</command>
				<property>controls/flight/elevator-trim-lever</property>
				<value>1</value>
			</binding>
			<mod-up>
