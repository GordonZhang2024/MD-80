		<or>
			<greater-than>
				<property>systems/electrical/bus/dc-l</property>
				<value>15</value>
			</greater-than>
			<greater-than>
				<property>systems/electrical/bus/dc-r</property>
				<value>15</value>
			</greater-than>
		</or>
	</and>
  </condition>
</animation>

