<PropertyList>
	
	<path>op.ac</path>
	
	<animation>
		<type>material</type>
		<object-name>Face</object-name>
		<object-name>Needle</object-name>
		<emission>
			<red>1.0</red>
			<green>0.6</green>
			<blue>0.3</blue>
			<factor-prop>systems/lighting/center-panel</factor-prop>
		</emission>
	</animation>
	
	<animation>
		<type>rotate</type>
		<object-name>Needle</object-name>
		<property>instrumentation/op[0]/op</property>
		<interpolation>
			<entry><ind>  0</ind><dep>-25</dep></entry>
			<entry><ind>100</ind><dep>225</dep></entry>
		</interpolation>
		<axis>
			<x>-1</x>
			<y>0</y>
			<z>0</z>
		</axis>
	</animation>

</PropertyList>
