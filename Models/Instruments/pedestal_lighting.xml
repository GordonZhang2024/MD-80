<PropertyList>
	
	<path>pedestal_lighting.ac</path>
	
	<animation>
		<type>material</type>
		<object-name>Integrity</object-name>
		<emission>
			<red>1.0</red>
			<green>0.6</green>
			<blue>0.3</blue>
			<factor-prop>controls/lighting/pedestal-panel-norm</factor-prop>
		</emission>
	</animation>
	
	<animation>
		<type>knob</type>
		<object-name>KnobPanel</object-name>
		<object-name>IntegrityPanel</object-name>
		<property>controls/lighting/pedestal-panel-knb</property>
		<interpolation>
			<entry><ind>0</ind><dep>-135</dep></entry>
			<entry><ind>1</ind><dep> 135</dep></entry>
		</interpolation>
		<center>
			<x-m>0.00757</x-m>
			<y-m>-0.0279</y-m>
			<z-m>-0.006946</z-m>
		</center>
		<axis>
			<x>-1</x>
			<y>0</y>
			<z>0</z>
		</axis>
		<action>
			<binding>
				<command>property-adjust</command>
				<property>controls/lighting/pedestal-panel-knb</property>
				<factor>0.05</factor>
				<min>0</min>
				<max>1</max>
				<wrap>false</wrap>
			</binding>
		</action>
	</animation>
	
	<animation>
		<type>knob</type>
		<object-name>KnobPanelTop</object-name>
		<object-name>IntegrityPanelTop</object-name>
		<property>controls/lighting/center-panel-knb</property>
		<interpolation>
			<entry><ind>0</ind><dep>-135</dep></entry>
			<entry><ind>1</ind><dep> 135</dep></entry>
		</interpolation>
		<center>
			<x-m>0.00757</x-m>
			<y-m>-0.0278</y-m>
			<z-m>-0.00646</z-m>
		</center>
		<axis>
			<x>-1</x>
			<y>0</y>
			<z>0</z>
		</axis>
		<action>
			<binding>
				<command>property-adjust</command>
				<property>controls/lighting/center-panel-knb</property>
				<factor>0.05</factor>
				<min>0</min>
				<max>1</max>
				<wrap>false</wrap>
			</binding>
		</action>
	</animation>
	
	<animation>
		<type>knob</type>
		<object-name>KnobDigital</object-name>
		<object-name>IntegrityDigital</object-name>
		<property>controls/lighting/pedestal-digital-knb</property>
		<interpolation>
			<entry><ind>0</ind><dep>-135</dep></entry>
			<entry><ind>1</ind><dep> 135</dep></entry>
		</interpolation>
		<center>
			<x-m>0.00759</x-m>
			<y-m>0.00024</y-m>
			<z-m>-0.00704</z-m>
		</center>
		<axis>
			<x>-1</x>
			<y>0</y>
			<z>0</z>
		</axis>
		<action>
			<binding>
				<command>property-adjust</command>
				<property>controls/lighting/pedestal-digital-knb</property>
				<factor>0.05</factor>
				<min>0</min>
				<max>1</max>
				<wrap>false</wrap>
			</binding>
		</action>
	</animation>
	
	<animation>
		<type>rotate</type>
		<object-name>KnobFlood</object-name>
		<object-name>IntegrityFlood</object-name>
		<property>controls/lighting/pedestal-flood-knb</property>
		<interpolation>
			<entry><ind>0</ind><dep>-135</dep></entry>
			<entry><ind>1</ind><dep> 135</dep></entry>
		</interpolation>
		<center>
			<x-m>0.00757</x-m>
			<y-m>0.02804</y-m>
			<z-m>-0.006946</z-m>
		</center>
		<axis>
			<x>-1</x>
			<y>0</y>
			<z>0</z>
		</axis>
	</animation>

</PropertyList>
