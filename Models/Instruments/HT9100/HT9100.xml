<PropertyList>
	
	<path>HT9100.ac</path>
	
	<animation>
		<type>material</type>
		<object-name>LightingGroup</object-name>
		<emission>
			<red>1.0</red>
			<green>0.6</green>
			<blue>0.3</blue>
			<factor-prop>systems/lighting/pedestal-panel</factor-prop>
		</emission>
	</animation>
	
	<animation>
		<type>select</type>
		<object-name>Call</object-name>
		<object-name>ExecLight</object-name>
		<object-name>Msg</object-name>
		<condition>
			<equals>
				<property>systems/fms/power-avail</property>
				<value>1</value>
			</equals>
		</condition>
	</animation>

</PropertyList>
