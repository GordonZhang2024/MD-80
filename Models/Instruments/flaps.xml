<PropertyList>
	
	<path>flaps.ac</path>
	
	<animation>
		<type>material</type>
		<object-name>Face</object-name>
		<object-name>NeedleL</object-name>
		<object-name>NeedleR</object-name>
		<emission>
			<red>1.0</red>
			<green>0.6</green>
			<blue>0.3</blue>
			<factor-prop>controls/lighting/center-panel-norm</factor-prop>
		</emission>
	</animation>
	
	<animation>
		<type>rotate</type>
		<object-name>NeedleL</object-name>
		<property>instrumentation/flap-slat/flap-l</property>
		<interpolation>
			<entry><ind> 0</ind><dep>  0</dep></entry>
			<entry><ind>40</ind><dep>258</dep></entry>
		</interpolation>
		<axis>
			<x>-1</x>
			<y>0</y>
			<z>0</z>
		</axis>
	</animation>
	
	<animation>
		<type>rotate</type>
		<object-name>NeedleR</object-name>
		<property>instrumentation/flap-slat/flap-r</property>
		<interpolation>
			<entry><ind> 0</ind><dep>  0</dep></entry>
			<entry><ind>40</ind><dep>258</dep></entry>
		</interpolation>
		<axis>
			<x>-1</x>
			<y>0</y>
			<z>0</z>
		</axis>
	</animation>

</PropertyList>

