<PropertyList>
	
	<path>brake-press.ac</path>
	
	<animation>
		<type>material</type>
		<object-name>Face</object-name>
		<object-name>NeedleL</object-name>
		<object-name>NeedleR</object-name>
		<emission>
			<red>1.0</red>
			<green>0.6</green>
			<blue>0.3</blue>
			<factor-prop>systems/lighting/capt-panel</factor-prop>
		</emission>
	</animation>
	
	<animation>
		<type>rotate</type>
		<object-name>NeedleL</object-name>
		<property>systems/hydraulics/brakes/sys-l-psi-needle</property>
		<interpolation>
			<entry><ind>   0</ind><dep>  0</dep></entry>
			<entry><ind>4000</ind><dep>320</dep></entry>
		</interpolation>
		<axis>
			<x>-1</x>
			<y>0</y>
			<z>0</z>
		</axis>
	</animation>
	
	<animation>
		<type>rotate</type>
		<object-name>NeedleR</object-name>
		<property>systems/hydraulics/brakes/sys-r-psi-needle</property>
		<interpolation>
			<entry><ind>   0</ind><dep>  0</dep></entry>
			<entry><ind>4000</ind><dep>320</dep></entry>
		</interpolation>
		<axis>
			<x>-1</x>
			<y>0</y>
			<z>0</z>
		</axis>
	</animation>

</PropertyList>

