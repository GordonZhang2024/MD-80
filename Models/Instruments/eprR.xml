<PropertyList>
	
	<path>epr.ac</path>
	
	<animation>
		<type>material</type>
		<object-name>Face</object-name>
		<object-name>Needle</object-name>
		<object-name>eprLimit</object-name>
		<object-name>eprDigits</object-name>
		<emission>
			<red>1.0</red>
			<green>0.6</green>
			<blue>0.3</blue>
			<factor-prop>controls/lighting/main-panel-norm</factor-prop>
		</emission>
	</animation>
	
	<animation>
		<type>rotate</type>
		<object-name>Needle</object-name>
		<property>instrumentation/epr[1]/epr</property>
		<interpolation>
			<entry><ind>0.8</ind><dep>  0</dep></entry>
			<entry><ind>2.5</ind><dep>255</dep></entry>
		</interpolation>
		<axis>
			<x>-1</x>
			<y>0</y>
			<z>0</z>
		</axis>
	</animation>
	
	<animation>
		<type>rotate</type>
		<object-name>eprLimit</object-name>
		<property>instrumentation/epr[1]/epr-bug</property>
		<interpolation>
			<entry><ind>0.8</ind><dep>  0</dep></entry>
			<entry><ind>2.5</ind><dep>255</dep></entry>
		</interpolation>
		<axis>
			<x>-1</x>
			<y>0</y>
			<z>0</z>
		</axis>
	</animation>
	
	<animation>
		<type>textranslate</type>
		<object-name>epr1</object-name>
		<property>instrumentation/epr[1]/epr-d100</property>
		<factor>0.0695</factor>
		<step>1</step>
		<scroll>0.01</scroll>
		<axis>
			<x>0</x>
			<y>1</y>
			<z>0</z>
		</axis>
	</animation>
	
	<animation>
		<type>textranslate</type>
		<object-name>epr2</object-name>
		<property>instrumentation/epr[1]/epr-d10</property>
		<factor>0.0695</factor>
		<step>1</step>
		<scroll>0.1</scroll>
		<axis>
			<x>0</x>
			<y>1</y>
			<z>0</z>
		</axis>
	</animation>
	
	<animation>
		<type>textranslate</type>
		<object-name>epr3</object-name>
		<property>instrumentation/epr[1]/epr-d1</property>
		<factor>0.0695</factor>
		<step>0.01</step>
		<axis>
			<x>0</x>
			<y>1</y>
			<z>0</z>
		</axis>
	</animation>

</PropertyList>
