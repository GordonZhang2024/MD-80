<PropertyList>
	
	<path>rearlogo.ac</path>
	<nopreview/>
	
	<animation>
		<type>noshadow</type>
		<object-name>tailnavlightals</object-name>
	</animation>
	
	<effect>
		<inherits-from>Aircraft/A320-family/Models/lights/ALS/rearlogoR</inherits-from>
		<object-name>tailnavlightals</object-name>
	</effect>
	
	<animation>
		<type>scale</type>
		<x-offset>0.2</x-offset>
		<y-offset>0.2</y-offset>
		<z-offset>0.2</z-offset>
	</animation>
	
	<animation>
		<type>dist-scale</type>
		<object-name>tailnavlightals</object-name>
		<interpolation>
			<entry>
				<ind>0</ind>
				<dep>1</dep>
			</entry>
			<entry>
				<ind>300</ind>
				<dep>4</dep>
			</entry>
			<entry>
				<ind>1500</ind>
				<dep>8</dep>
			</entry>
		</interpolation>
	</animation>

</PropertyList>
