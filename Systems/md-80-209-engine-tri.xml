						<value>0.001</value>
					</max>
				</quotient>
			</function>
		</fcs_function>
		
		<fcs_function name="engine/limit/man-2-norm-input"> <!-- Opposite of EPR Calc -->
			<function>
				<pow>
					<table>
						<independentVar lookup="row">engine/limit/man-2-norm-ref</independentVar>
						<tableData>
							0.00     24743.1
							0.03     81455.7
							1.00  10571289.8
						</tableData>
					</table>
					<quotient>
						<value>1</value>
						<value>3.5</value>
					</quotient>
				</pow>
			</function>
		</fcs_function>
		
		<fcs_function name="engine/limit/man-2-norm">
			<function>
				<table>
					<independentVar lookup="row">engine/limit/man-2-norm-input</independentVar>
					<tableData>
						 25.3  0
						101.6  1
					</tableData>
				</table>
			</function>
		</fcs_function>
		
