							<property>/instrumentation/epr[1]/epr-input</property> <!-- Not epr-fixed on purpose! -->
							<value>10</value>
						</product>
					</fraction>
					<value>10</value>
				</product>
			</function>
		</fcs_function>
		
		<switch name="/instrumentation/epr[1]/epr-bug-input">
			<default value="/instrumentation/epr[1]/epr-bug-input"/>
			<test value="engine/limit/active-ind-cmd">
